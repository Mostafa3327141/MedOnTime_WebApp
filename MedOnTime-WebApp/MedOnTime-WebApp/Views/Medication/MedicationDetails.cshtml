@model Medication

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h1>Details</h1>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Medication ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MedID)
        </dd>
        <dt class="col-sm-2">
            Medication Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MedicationName)
        </dd>
        <dt class="col-sm-2">
            Method of Taking
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MethodOfTaking)
        </dd>
        <dt class="col-sm-2">
            Medication Type
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MedicationType)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "MedicationEdit", new { medID = Model.MedID }) |
    @Html.ActionLink("Delete", "MedicationDelete", new { medID = Model.MedID }) |
    @Html.ActionLink("Back to list", "MedicationList")
</div>
