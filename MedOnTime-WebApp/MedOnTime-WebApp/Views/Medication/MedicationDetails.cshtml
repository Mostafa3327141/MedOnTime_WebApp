@model Medication

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h1>Details</h1>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Medication ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            Caretaker ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CaretakerID)
        </dd>
        <dt class="col-sm-2">
            Patient ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PatientID)
        </dd>
        <dt class="col-sm-2">
            Medication Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MedicationName)
        </dd>
        <dt class="col-sm-2">
            Method of Taking
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MethodOfTaking)
        </dd>
        <dt class="col-sm-2">
            Medication Type
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MedicationType)
        </dd>
        <dt class="col-sm-2">
            Quantity
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Quantity)
        </dd>
        <dt class="col-sm-2">
            Time Of Taking
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TimeOfTaking)
        </dd>
        <dt class="col-sm-2">
            Frequency
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Frequency)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "MedicationEdit", new { Id = Model.Id }) |
    @Html.ActionLink("Delete", "MedicationDelete", new { Id = Model.Id }) |
    @Html.ActionLink("Back to list", "MedicationList")
</div>
